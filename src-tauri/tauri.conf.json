{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sqs-dashboard",
  "version": "1.2.4",
  "identifier": "es.miguel.sqs-dashboard",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Simple Queue Dashboard",
        "width": 800,
        "height": 600,
        "minWidth": 700,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
